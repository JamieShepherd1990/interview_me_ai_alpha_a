{
  "expo": {
    "name": "InterviewCoach AI",
    "slug": "interview-coach",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.interviewcoach.ai",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "InterviewCoach AI needs access to your microphone to conduct voice-based interviews and provide real-time feedback.",
        "NSCameraUsageDescription": "InterviewCoach AI may need camera access for future video interview features."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.interviewcoach.ai",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-av",
        {
          "microphonePermission": "InterviewCoach AI needs microphone access to conduct voice-based interviews and provide real-time coaching feedback."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b232ee48-e40e-48c8-b57b-51dcb981a541"
      }
    },
    "owner": "jamieshepherd90"
  }
}
